<h2>Results index</h2>

<table class="table table-hover table-inverse">
  <% if @range == "name" %>
    <tbody>
      <% @item_names.each do |user| %>
        <tr>
          <td><%= user.name %></td>
        </tr>
      <% end %>
    </tbody>
  <% else %>
    <tbody>
      <% @maker_names.each do |book| %>
        <tr>
          <td><%= book.title %></td>
          <td><%= book.body %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>